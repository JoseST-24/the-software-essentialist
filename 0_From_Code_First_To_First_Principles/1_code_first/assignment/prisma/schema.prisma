// schema.prisma

generator client {
  provider = "prisma-client-js"
}

// Define your data model here
model User {
  id        Int    @id @default(autoincrement())
  email     String @unique
  username  String
  firstName String
  lastName  String
  password  String
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
